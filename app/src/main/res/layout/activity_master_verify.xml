<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white"
    tools:context=".ui.mine.MasterVerifyActivity">
    <include layout="@layout/toolbar_layout"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="@color/background">
        <RelativeLayout
            android:id="@+id/face_rl"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_44"
            android:background="@drawable/item_selector"
            android:layout_marginTop="@dimen/dp_20">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="真实头像"
                android:textColor="@color/text_drak"
                android:textSize="@dimen/sp_15"
                android:layout_marginLeft="@dimen/common_margin"
                android:layout_marginRight="@dimen/common_margin"
                android:layout_centerVertical="true"/>
            <ImageView
                android:id="@+id/view1"
                android:layout_width="@dimen/common_margin"
                android:layout_height="@dimen/common_margin"
                android:src="@drawable/arrow_right_black"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/common_margin"/>
            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/face_iv"
                android:layout_width="@dimen/dp_30"
                android:layout_height="@dimen/dp_30"
                android:layout_toLeftOf="@+id/view1"
                android:src="@drawable/moren_face"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/common_margin"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/border3"
                android:layout_alignParentBottom="true"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/card_pic_rl"
            android:background="@drawable/item_selector"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_45">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="证件照片"
                android:textColor="@color/text_drak"
                android:textSize="@dimen/sp_15"
                android:layout_marginLeft="@dimen/common_margin"
                android:layout_marginRight="@dimen/common_margin"
                android:layout_centerVertical="true"/>
            <ImageView
                android:id="@+id/view3"
                android:layout_width="@dimen/common_margin"
                android:layout_height="@dimen/common_margin"
                android:src="@drawable/arrow_right_black"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/common_margin"/>
            <TextView
                android:id="@+id/pic_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toLeftOf="@+id/view3"
                android:text="未上传"
                android:textSize="@dimen/sp_15"
                android:textColor="@color/blue"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/common_margin"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/border3"
                android:layout_alignParentBottom="true"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/name_rl"
            android:background="@drawable/item_selector"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_45">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="姓名"
                android:textColor="@color/text_drak"
                android:textSize="@dimen/sp_15"
                android:layout_marginLeft="@dimen/common_margin"
                android:layout_marginRight="@dimen/common_margin"
                android:layout_centerVertical="true"/>
            <TextView
                android:id="@+id/name_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/sp_15"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/common_margin"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/border3"
                android:layout_alignParentBottom="true"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/card_num_rl"
            android:background="@drawable/item_selector"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_45">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="证件号码"
                android:textColor="@color/text_drak"
                android:textSize="@dimen/sp_15"
                android:layout_marginLeft="@dimen/common_margin"
                android:layout_marginRight="@dimen/common_margin"
                android:layout_centerVertical="true"/>
            <TextView
                android:id="@+id/card_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/sp_15"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/common_margin"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/border3"
                android:layout_alignParentBottom="true"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/card_date_rl"
            android:background="@drawable/item_selector"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_45">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="证件有效期"
                android:textColor="@color/text_drak"
                android:textSize="@dimen/sp_15"
                android:layout_marginLeft="@dimen/common_margin"
                android:layout_marginRight="@dimen/common_margin"
                android:layout_centerVertical="true"/>
            <TextView
                android:id="@+id/date_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/sp_15"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/common_margin"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/border3"
                android:layout_alignParentBottom="true"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/zhicheng_rl"
            android:background="@drawable/item_selector"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_45"
            android:layout_marginTop="@dimen/dp_20">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="职称"
                android:textColor="@color/text_drak"
                android:textSize="@dimen/sp_15"
                android:layout_marginLeft="@dimen/common_margin"
                android:layout_marginRight="@dimen/common_margin"
                android:layout_centerVertical="true"/>
            <ImageView
                android:id="@+id/view4"
                android:layout_width="@dimen/common_margin"
                android:layout_height="@dimen/common_margin"
                android:src="@drawable/arrow_right_black"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/common_margin"/>
            <TextView
                android:id="@+id/zhicheng_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toLeftOf="@+id/view4"
                android:text="未完善"
                android:textSize="@dimen/sp_15"
                android:textColor="@color/blue"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/common_margin"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/border3"
                android:layout_alignParentBottom="true"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/jieshao_rl"
            android:background="@drawable/item_selector"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_45">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="个人介绍"
                android:textColor="@color/text_drak"
                android:textSize="@dimen/sp_15"
                android:layout_marginLeft="@dimen/common_margin"
                android:layout_marginRight="@dimen/common_margin"
                android:layout_centerVertical="true"/>
            <ImageView
                android:id="@+id/view5"
                android:layout_width="@dimen/common_margin"
                android:layout_height="@dimen/common_margin"
                android:src="@drawable/arrow_right_black"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/common_margin"/>
            <TextView
                android:id="@+id/jieshao_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toLeftOf="@+id/view5"
                android:text="未完善"
                android:textSize="@dimen/sp_15"
                android:textColor="@color/blue"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/common_margin"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/border3"
                android:layout_alignParentBottom="true"/>
        </RelativeLayout>
    </LinearLayout>
</LinearLayout>